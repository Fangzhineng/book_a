<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- HeaderBar自定义属性 -->
    <declare-styleable name="HeaderBar">
        <attr name="isShowBack" format="boolean"/>
        <attr name="isShowRight" format="boolean"/>
        <attr name="titleText" format="string"/>
        <attr name="titleColor" format="color"/>
        <attr name="rightText" format="string"/>
        <attr name="bar_background" format="color"/>

    </declare-styleable>

    <!-- LabelText自定义属性 -->
    <declare-styleable name="LabelText">
        <attr name="labelText" format="string"/>
        <attr name="contentText" format="string"/>
    </declare-styleable>


    <declare-styleable name="MainNavigateTabBar">
        <attr name="navigateTabTextColor" format="color|reference"/>
        <attr name="navigateTabSelectedTextColor" format="color|reference"/>
        <attr name="containerId" format="integer|reference"/>
        <attr name="navigateTabTextSize" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="radius" format="reference|dimension" />
    </declare-styleable>
</resources>
